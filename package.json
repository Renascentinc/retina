{
  "name": "retina",
  "displayName": "RETINA",
  "version": "3.0.0",
  "description": "The Renascent inc. Asset Tracking Application",
  "author": "Renascent inc.",
  "private": true,
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "generate-icons": "vsvg -s ./svg-icons -t ./src/assets/icons/svg",
    "start": "changeFile=false parcel index.html --port 4200",
    "clean-start": "rm -rf .cache && yarn start",
    "test": "eslint --ext .js,.vue src",
    "build": "changeFile=false parcel build index.html --no-source-maps",
    "build-cordova": "parcel build index.html --out-dir=www --public-url=./ --no-minify",
    "android": "yarn build-cordova && cordova prepare && cordova run android",
    "ios": "yarn build-cordova && cordova prepare && ./scripts/setup-ios-splash.sh && cordova run ios --buildFlag='-UseModernBuildSystem=0'"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^1.5.5",
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.4.2",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "cordova-android": "^7.0.0",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-printer": "^0.7.3",
    "fastclick": "https://github.com/Renascentinc/fastclick.git",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "html2pdf.js": "^0.9.0",
    "phone-number-formats": "^2.2.0",
    "sweetalert2": "^7.29.1",
    "v-calendar": "^0.9.7",
    "vee-validate": "^2.1.0-beta.11",
    "vue-apollo": "^3.0.0-beta.25",
    "vue-avatar": "^2.1.6",
    "vue-drawer-layout": "^0.3.2",
    "vue-js-modal": "^1.3.26",
    "vue-lazyload": "^1.2.6",
    "vue-mq": "^0.2.1",
    "vue-notifications": "^0.9.0",
    "vue-svgicon": "^3.2.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@vue/component-compiler-utils": "^2.1.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-change-file": "^1.3.0",
    "phonegap-nfc": "^1.0.3",
    "sass": "^1.14.3",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "phonegap-nfc": {
        "NFC_USAGE_DESCRIPTION": ""
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-device": {},
      "cordova-plugin-printer": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "parcel-plugin-change-file": {}
}
