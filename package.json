{
  "name": "retina",
  "version": "3.1.0",
  "private": true,
  "description": "Renascent inc. Asset Tracking Application",
  "author": "Renascent Inc.",
  "scripts": {
    "setup": "yarn && mkdir www plugins platforms && cordova prepare && yarn mobile:assets",
    "start": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode=development",
    "mobile:assets": "app-icon generate -i public/static/icon-ios.png --platforms=ios && app-icon generate -i public/static/icon-android.png --platforms=android && cordova-splash --splash=public/static/launch.png",
    "build:mobile": "VUE_APP_PLATFORM=cordova vue-cli-service build",
    "build:mobile:dev": "VUE_APP_PLATFORM=cordova vue-cli-service build --mode=development",
    "android": "yarn build:cordova && cordova build android",
    "ios": "yarn build:cordova && cordova build ios",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "clean:lock": "yarn-deduplicate yarn.lock"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^1.5.5",
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.4.2",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "browser-image-compression": "^0.0.3",
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-printer": "^0.7.3",
    "debounce": "^1.2.0",
    "fastclick": "https://github.com/Renascentinc/fastclick.git",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "html2pdf.js": "^0.9.0",
    "phone-number-formats": "^2.2.0",
    "register-service-worker": "^1.6.2",
    "sweetalert2": "^7.29.2",
    "v-calendar": "^0.9.7",
    "v-money": "^0.8.1",
    "vee-validate": "^2.1.0-beta.11",
    "vue": "^2.6.6",
    "vue-apollo": "^3.0.0-beta.25",
    "vue-avatar": "^2.1.6",
    "vue-drawer-layout": "^1.0.0",
    "vue-infinite-scroll": "^2.0.2",
    "vue-mq": "^0.2.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "app-icon": "^0.12.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cordova": "^9.0.0",
    "cordova-splash": "^1.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "vue-template-compiler": "^2.6.9",
    "yarn-deduplicate": "^1.1.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "phonegap-nfc": {
        "NFC_USAGE_DESCRIPTION": ""
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-device": {},
      "cordova-plugin-printer": {},
      "cordova-plugin-statusbar": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}