{
  "name": "retina",
  "version": "3.0.0",
  "description": "The Renascent inc. Asset Tracking Application",
  "author": "Renascent inc.",
  "private": true,
  "scripts": {
    "start": "parcel index.html --port 4200",
    "test": "jest --config test/unit/jest.conf.js --coverage",
    "build": "parcel build index.html",
    "build-cordova": "node parcel-build.js && cp cordova/index.html cordova/www/",
    "android": "npm run build-cordova && cd cordova && cordova run android",
    "ios": "npm run build-cordova && cd cordova && cordova run ios"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "parcel-bundler": "^1.7.0",
    "vue-jest": "^2.4.1",
    "vue-template-compiler": "^2.5.16"
  }
}
