{
  "name": "retina",
  "displayName": "RETINA",
  "version": "3.0.0",
  "description": "The Renascent inc. Asset Tracking Application",
  "author": "Renascent inc.",
  "private": true,
  "scripts": {
    "start": "changeFile=false parcel index.html --port 4200",
    "test": "eslint --ext .js,.vue src",
    "build": "changeFile=false parcel build index.html --no-source-maps",
    "build-cordova": "changeFile=true parcel build index.html --out-dir=www --public-url=./ --no-source-maps --no-minify",
    "android": "npm run build-cordova && cordova prepare && cordova run android",
    "ios": "npm run build-cordova && cordova prepare && ./scripts/setup-ios-splash.sh && cordova run ios",
    "cordova-browser": "npm run build cordova && cordova serve 4200"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^1.5.5",
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.4.2",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "fastclick": "^1.0.6",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "gsap": "^2.0.1",
    "v-calendar": "^0.9.7",
    "vue-apollo": "^3.0.0-beta.25",
    "vue-avatar": "^2.1.6",
    "vue-drawer-layout": "^0.3.2",
    "vue-lazyload": "^1.2.6",
    "vue-select": "^2.5.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.1.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "cordova-android": "^7.0.0",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.7.0",
    "parcel-plugin-sw-precache": "^0.5.2",
    "phonegap-nfc": "^1.0.3",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-jest": "^2.4.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "phonegap-nfc": {
        "NFC_USAGE_DESCRIPTION": ""
      },
      "cordova-plugin-splashscreen": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "parcel-plugin-change-file": {}
}
